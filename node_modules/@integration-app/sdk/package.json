{
  "name": "@integration-app/sdk",
  "version": "1.9.0",
  "description": "JavaScript SDK for Integration.app",
  "author": "Integration.app",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.mjs",
  "unpkg": "./dist/bundle.js",
  "exports": {
    "import": {
      "types": "./dist/index.module.d.mts",
      "default": "./dist/index.module.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "browser": {
    "./src/index.ts": "./dist/index.browser.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:bundle && npm run build:dts",
    "build:bundle": "rollup -c rollup.config.mjs",
    "build:dts": "rollup -c rollup.dts.config.mjs",
    "dev": "turbo run dev:bundle dev:dts --parallel",
    "dev:bundle": "rollup -c rollup.config.mjs --watch",
    "dev:dts": "rollup -c rollup.dts.config.mjs --watch --waitForBundleInput",
    "docs": "typedoc --plugin typedoc-plugin-missing-exports --tsconfig tsconfig.json",
    "docs:dev": "typedoc --plugin typedoc-plugin-missing-exports --tsconfig tsconfig.json --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "test:package": "cd test-package && npm run test && cd ..",
    "version": "npm version"
  },
  "devDependencies": {
    "@integration-app/eslint-config": "*",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/insert-css": "^2.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.8.6",
    "axios-mock-adapter": "^2.1.0",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.36.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.2.6",
    "typedoc": "^0.26.11",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@ungap/structured-clone": "^1.3.0",
    "axios": "^1.8.4",
    "fast-deep-equal": "^3.1.3",
    "humanparser": "^2.7.0",
    "insert-css": "^2.0.0",
    "js-convert-case": "^4.2.0",
    "jsonata": "^1.8.7",
    "penpal": "^6.2.2",
    "qs": "^6.14.0",
    "url-join": "^4.0.1",
    "zod": "^3.24.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "json",
      "js"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
